  <entry>
    <title>流程控制</title>
    <link href="http://example.com/post/%E6%B5%81%E7%A8%8B%E6%8E%A7%E5%88%B6/"/>
    <id>http://example.com/post/%E6%B5%81%E7%A8%8B%E6%8E%A7%E5%88%B6/</id>
